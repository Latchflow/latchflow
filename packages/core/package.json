{
  "name": "@latchflow/core",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "nodemon --watch src --ext ts --exec \"tsx src/index.ts\"",
    "test": "vitest run && vitest run -c vitest.e2e.config.ts",
    "test:unit": "vitest run src",
    "test:integration": "vitest run tests",
    "test:e2e": "vitest run -c vitest.e2e.config.ts",
    "lint": "eslint '{src,tests}/**/*.ts'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@aws-sdk/s3-request-presigner": "^3.883.0",
    "@latchflow/db": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.53.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-metrics": "^1.26.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.14",
    "pino": "^9.3.2",
    "pino-http": "^9.0.0",
    "pino-pretty": "^13.1.1",
    "zip-stream": "^6.0.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.0",
    "@types/nodemailer": "^6.4.15",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^8.56.0",
    "nodemon": "^3.1.10",
    "testcontainers": "^10.10.0",
    "tsup": "^8.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.4.0",
    "vitest": "^2.1.9"
  }
}
