{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "env": {"es2020": true, "node": true},
  "ignorePatterns": ["dist"],
  "rules": {
    "quotes": "off",
    "quote-props": ["error", "as-needed"]
  }
}
